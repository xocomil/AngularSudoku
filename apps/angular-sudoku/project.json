{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/angular-sudoku/src",
  "prefix": "angular-sudoku",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/angular-sudoku",
        "index": "apps/angular-sudoku/src/index.html",
        "main": "apps/angular-sudoku/src/main.ts",
        "polyfills": "apps/angular-sudoku/src/polyfills.ts",
        "tsConfig": "apps/angular-sudoku/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/angular-sudoku/src/favicon.ico",
          "apps/angular-sudoku/src/assets"
        ],
        "styles": [
          "apps/angular-sudoku/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/angular-sudoku/src/environments/environment.ts",
              "with": "apps/angular-sudoku/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "angular-sudoku:build:production"
        },
        "development": {
          "browserTarget": "angular-sudoku:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "angular-sudoku:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/angular-sudoku/**/*.ts",
          "apps/angular-sudoku/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/angular-sudoku"
      ],
      "options": {
        "jestConfig": "apps/angular-sudoku/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "cap": {
      "executor": "@nrwl/workspace:run-commands",
      "configurations": {
        "add-ios": {
          "command": "npx cap add ios",
          "cwd": "apps/angular-sudoku"
        },
        "add-android": {
          "command": "npx cap add android",
          "cwd": "apps/angular-sudoku"
        },
        "any": {
          "command": "npx cap {args.cmd}",
          "cwd": "apps/angular-sudoku"
        },
        "update": {
          "commands": [
            "nx build angular-sudoku",
            "npx cap update"
          ],
          "parallel": false,
          "cwd": "apps/angular-sudoku"
        },
        "sync": {
          "commands": [
            "nx build angular-sudoku",
            "npx cap sync"
          ],
          "parallel": false,
          "cwd": "apps/angular-sudoku"
        },
        "open-ios": {
          "command": "npx cap open ios",
          "cwd": "apps/angular-sudoku"
        },
        "open-android": {
          "command": "npx cap open android",
          "cwd": "apps/angular-sudoku"
        },
        "run-ios": {
          "command": "npx cap run ios",
          "cwd": "apps/angular-sudoku"
        },
        "run-android": {
          "command": "npx cap run android",
          "cwd": "apps/angular-sudoku"
        }
      },
      "defaultConfiguration": "any"
    }
  },
  "tags": []
}
