{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/components/src",
  "prefix": "sud",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/components"],
      "options": {
        "jestConfig": "libs/components/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/components/**/*.ts",
          "libs/components/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/components/.storybook",
        "browserTarget": "components:build-storybook",
        "compodoc": false
      },
      "styles": ["apps/angular-sudoku/src/styles.scss"],
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputDir": "dist/storybook/components",
        "configDir": "libs/components/.storybook",
        "browserTarget": "components:build-storybook",
        "compodoc": false
      },
      "styles": ["apps/angular-sudoku/src/styles.scss"],
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
